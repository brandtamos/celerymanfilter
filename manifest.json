{
    "name": "Celery Man Filter",
    "version": "0.0.3",
    "manifest_version": 2,
    "browser_action": {
        "default_title": "Click Here",
        "default_popup": "popup.html"
    },
    "permissions": [
        "storage"
    ],
    "content_scripts": [
        {
            "matches": ["https://www.facebook.com/*"],
            "js": ["thirdParty/jquery-2.1.4.min.js","content.js"]
        }
    ],
    "icons": {
        "128": "icon128.png"
    }
}